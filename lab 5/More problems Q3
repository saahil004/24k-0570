#include <stdio.h>

int main() {
    char plainText, cipherText, decipherText;
    printf("enter character to be encrypted: ");
    scanf(" %c", &plainText);
    cipherText = plainText ^ 22;
    decipherText = cipherText ^ 22;
    printf("encrypted: %c", cipherText);
    printf("decrypted: %c", decipherText);
    return 0;
}
