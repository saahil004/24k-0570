#include <stdio.h>

int main() {
    int num;
    printf("enter num: ");
    scanf("%d", &num);
    if (num == 0) {
        printf("zero\n");
    } else if (num < 0) {
        printf("negative\n");
    } else if (num > 0) {
        (num % 2 == 0) ? printf("positive and even\n") : printf("positive and odd\n");
    }
    return 0;
}
