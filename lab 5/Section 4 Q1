#include <stdio.h>

int main() {
    int a;
    int b;
    printf("a=  ");
    scanf("%d",&a);
    printf("b=  ");
    scanf("%d",&b);
    printf("before swapping\na = %d\nb = %d\n", a, b);
    a = a ^ b;
    b = a ^ b;
    a = a ^ b;
    printf("after swapping\na = %d\nb = %d", a, b);
    return 0;
}
