#include <stdio.h>

int main() {
    int a, b, c;
    printf("Please enter a:  ");
    scanf("%d",&a);
    printf("Please enter b:  ");
    scanf("%d",&b);
    printf("Please enter c:  ");
    scanf("%d",&c);
    if (a > b) {
        if (a > c) {
            printf("a is the largest");
        } else {
            printf("c is the largest");
        }
    } else { 
        if (b > c) {
            printf("b is the largest");
        } else {
            printf("c is the largest");
        }
    }
    return 0;
}
