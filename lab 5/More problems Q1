#include <stdio.h>

int main() {
    int a = 7, b = 2, c = 4;
    if (a > b) {
        if (a > c) {
            printf("a is the largest");
        } else {
            printf("c is the largest");
        }
    } else { 
        if (b > c) {
            printf("b is the largest");
        } else {
            printf("c is the largest");
        }
    }
    return 0;
}
