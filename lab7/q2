#include <stdio.h>



int main() {
    int size;
    printf("enter the size of array: ");
    scanf("%d", &size);
    int arr[size];
    for (int i = 0; i < size; i++) {
        printf("\nEnter array element of index %d: ", i);
        scanf("%d", &arr[i]);
    }
    printf("The array in reverse order: \n");
    for (int i = size - 1; i >= 0; i--) {
        printf(" %d ", arr[i]);
    }
    return 0;
}
