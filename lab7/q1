#include <stdio.h>



int main() {
    int size;
    printf("enter the size of array: ");
    scanf("%d", &size);
    int arr[size];
    for (int i = 0; i < size; i++) {
        printf("\nEnter array element of index %d: ", i);
        scanf("%d", &arr[i]);
    }
    int sum = 0;
    for (int i = 0; i < size; i++) {
        sum += arr[i];
    }
    printf("The sum of elements is %d", sum);
    return 0;
}
