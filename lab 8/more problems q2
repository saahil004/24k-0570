#include <stdio.h>

int main() {
    int range;
    printf("Enter range: ");
    scanf("%d", &range);
    for (int i = range; i > 0; i--) {
       for (int j = i; j > 0; j--) {    
         if (i % 2 == 1) {
            if (j % 2 == 1) { 
             printf(" %d ", j);
            } 
         }
       }
       printf("\n");
    }
    return 0;
}
